<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <div>
        <a href="/">My Gallery</a>
        <a href="/addNew">Add</a>
    </div>
    <section class="grid">
        <%= %>
            <% result.forEach(element=> { %>
                <a href=<%=`/product/${element._id} `%>>
                    <img src=<%=element.ProductLink%> alt="">
                    <h1>
                        <%=element.Price %>
                    </h1>
                    <h1>
                        <%=element.Company %>
                    </h1>
                </a>
                <% }) %>

    </section>
</body>

</html>